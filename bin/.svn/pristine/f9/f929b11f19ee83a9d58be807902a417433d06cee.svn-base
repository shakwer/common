package com.fang.common.service.vo;

/**
 * <p>
 * Description: FangOaAuthenResult
 * </p>
 *
 * @author jinshilei
 *         2017年4月1日
 * @version 1.0
 *
 */
public class FangOaAuthenResult {

  /**
   * 结果编码(0为具有权限)
   */
  private String code;

  /**
   * 当前用户的OA邮箱(不带后缀，如：jinshilei)
   */
  private String loginid;

  /**
   * 资源id
   */
  private int resourceid;

  /**
   * 结果描述信息
   */
  private String msg;

  /**
   *
   * getCode
   *
   * @return 返回值
   */
  public String getCode() {
    return code;
  }

  /**
   *
   * setCode
   *
   * @param code
   *        结果编码
   */
  public void setCode(String code) {
    this.code = code;
  }

  /**
   *
   * getLoginid
   *
   * @return 返回值
   */
  public String getLoginid() {
    return loginid;
  }

  /**
   *
   * setLoginid
   *
   * @param loginid
   *        登录邮箱
   */
  public void setLoginid(String loginid) {
    this.loginid = loginid;
  }

  /**
   *
   * getResourceid
   *
   * @return 返回值
   */
  public Integer getResourceid() {
    return resourceid;
  }

  /**
   *
   * setResourceid
   *
   * @param resourceid
   *        资源id
   */
  public void setResourceid(Integer resourceid) {
    this.resourceid = resourceid;
  }

  /**
   *
   * getMsg
   *
   * @return 返回值
   */
  public String getMsg() {
    return msg;
  }

  /**
   *
   * setMsg
   *
   * @param msg
   *        结果描述
   */
  public void setMsg(String msg) {
    this.msg = msg;
  }

}
